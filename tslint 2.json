{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended"],
	"jsRules": {},
	"linterOptions": {},
	"rules": {
		"arrow-parens": [true, "ban-single-arg-parens"],
		"indent": [true, "tabs", 4],
		"member-access": [true, "no-public"],
		"no-console": [false],
		"semicolon": [false, "always"],
		"trailing-comma": [
			true,
			{ "multiline": "always", "singleline": "never" }
		]
	},
	"rulesDirectory": []
}
